pipeline {
    agent any
    tools {
      maven "maven_360"
    }
    stages {
        stage('Checkout') {
            steps {
            git 'https://github.com/JagdishMane/simple-java-maven-app.git'
            }
        }
        stage('SonarQube Analysis') {
          steps {
              sh "mvn clean verify sonar:sonar -Dsonar.projectKey=simple-java-maven-app"
            }
        }
    }
}
